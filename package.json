    "repository": {
        "type": "git",
        "url": "https://azuredatamicrosite.azurewebsites.net/azuredata"
    },
    "scripts": {
        "build": "npm run prepare && cross-env NODE_ENV=dev webpack -d --config webpack/dev-webpack.config.js",
        "build:production": "npm run prepare:deployment && cross-env NODE_ENV=production webpack -p --config webpack/prod-webpack.config.js --progress",
        "build:staging": "npm run prepare:deployment && cross-env NODE_ENV=production webpack -p --config webpack/staging-webpack.config.js --progress",
        "sass": "ts-node scripts/sass.ts",
        "sass:deployment": "ts-node scripts/sass.ts --deployment",
        "prepare": "ts-node scripts/prepare.ts",
        "prepare:deployment": "ts-node scripts/prepare.ts --deployment",
        "clean": "ts-node scripts/clean.ts",
        "dev-server": "npm run prepare && cross-env cross-env node_modules/.bin/webpack-dev-server --config webpack/dev-webpack.config.js --progress ",
        "npm:clean:react": "npm run clean -- --npmPackage --react",
        "npm:publish:react": "cross-env npm run npm:clean:react && cross-env gulp buildNpmPackage --react && cross-env npm publish packages/react",
        "server": "node scripts/server.js",
        "start": "npm run dev-server",
        "precommit": "lint-staged",
        "test:integration": "jest --config jest.config.integration.json"
    },
